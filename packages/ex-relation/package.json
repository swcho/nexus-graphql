{
  "name": "ex-relation",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "petstore:run": "docker run -d -e DISABLE_OAUTH=1 -e OPENAPI_BASE_PATH=/v3 -p 80:8080 openapitools/openapi-petstore",
    "gen": "yarn gen:preprocess && yarn gen:openapi-to-graphql && yarn gen:openapi-to-tslib && yarn gen:graphql-to-types && yarn gen:nexus-graphql",
    "gen:preprocess": "ts-node tools/oas3-preprocess.ts -i tools/oas3-samples/loopback-example-relation.json -o src/openapi.json",
    "gen:openapi-to-graphql": "openapi-to-graphql --no-viewer src/openapi.json --save src/generated/graphql/schema.graphql",
    "gen:openapi-to-tslib": "openapi-generator generate --skip-validate-spec --template-dir tools/typescript-axios-templates -g typescript-axios -i src/openapi.json -o src/generated/tslib",
    "gen:graphql-to-types": "graphql-codegen --config codegen.yml",
    "gen:nexus-graphql": "nexus-graphql-generate --schema src/generated/graphql/schema.graphql --typepath ../types/types --output src/generated/nexus-graphql",
    "start": "concurrently 'yarn start:rest' 'yarn start:self'",
    "start:rest": "cd ../../examples/loopback-example-relations; yarn start",
    "start:self": "ts-node-dev --no-notify --respawn --transpileOnly ./src",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "graphql": "14.2.1",
    "graphql-yoga": "1.17.4",
    "nexus": "0.11.6",
    "nexus-graphql": "*",
    "nexus-graphql-generate": "*"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.3.1",
    "@graphql-codegen/introspection": "^1.3.1",
    "@graphql-codegen/typescript": "1.3.1",
    "@openapitools/openapi-generator-cli": "^0.0.16-4.0.3",
    "@types/graphql": "14.2.0",
    "@types/node": "^12.6.4",
    "api-spec-converter": "^2.8.3",
    "commander": "^2.20.0",
    "concurrently": "^4.1.1",
    "js-yaml": "^3.13.1",
    "openapi-to-graphql-cli": "^1.1.0",
    "openapi3-ts": "^1.3.0",
    "ts-node": "8.1.0",
    "ts-node-dev": "1.0.0-pre.32",
    "typescript": "3.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "printWidth": 80,
    "trailingComma": "all",
    "semi": false
  },
  "private": true
}
