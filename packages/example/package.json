    "build": "tsc",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "graphql": "14.2.1",
    "graphql-yoga": "1.17.4",
    "nexus": "0.11.6",
    "nexus-graphql": "0.0.1",
    "nexus-graphql-generate": "0.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.3.1",
    "@graphql-codegen/introspection": "^1.3.1",
    "@graphql-codegen/typescript": "1.3.1",
    "@types/graphql": "14.2.0",
    "@types/node": "10.14.5",
    "ts-node": "8.1.0",
    "ts-node-dev": "1.0.0-pre.32",
    "typescript": "3.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
